{
  "name": "LunaTV",
  "appid": "__UNI__LUNATV",
  "description": "LunaTV 影视聚合播放平台移动端应用",
  "versionName": "1.0.0",
  "versionCode": "100",
  "transformPx": false,
  "app-plus": {
    "usingComponents": true,
    "nvueStyleCompiler": "uni-app",
    "compilerVersion": 3,
    "compatible": {
      "ignoreVersion": true
    },
    "splashscreen": {
      "alwaysShowBeforeRender": true,
      "waiting": true,
      "autoclose": true,
      "delay": 0
    },
    "modules": {},
    "distribute": {
      "android": {
        "permissions": [
          "<uses-permission android:name=\"android.permission.INTERNET\"/>",
          "<uses-permission android:name=\"android.permission.ACCESS_NETWORK_STATE\"/>",
          "<uses-permission android:name=\"android.permission.ACCESS_WIFI_STATE\"/>",
          "<uses-permission android:name=\"android.permission.WRITE_EXTERNAL_STORAGE\"/>",
          "<uses-permission android:name=\"android.permission.READ_EXTERNAL_STORAGE\"/>"
        ],
        "minSdkVersion": 21,
        "targetSdkVersion": 30,
        "screenOrientation": ["portraitPrimary"]
      },
      "ios": {
        "idfa": false,
        "allowsArbitraryLoads": true,
        "screenOrientation": ["portrait"]
      },
      "sdkConfigs": {}
    },
    "networkTimeout": {
      "request": 30000,
      "connectSocket": 30000,
      "uploadFile": 30000,
      "downloadFile": 30000
    },
    "statusbar": {
      "immersed": true,
      "style": "dark",
      "background": "#0f0f1a"
    },
    "networkSecurity": {
      "domainWhitelist": [
        "doubanio.com",
        "img1.doubanio.com",
        "img2.doubanio.com",
        "img3.doubanio.com",
        "img4.doubanio.com",
        "img5.doubanio.com",
        "img6.doubanio.com",
        "img7.doubanio.com",
        "img9.doubanio.com"
      ]
    }
  },
  "quickapp": {},
  "mp-weixin": {
    "appid": "",
    "setting": {
      "urlCheck": false,
      "es6": true,
      "postcss": true,
      "minified": true
    },
    "usingComponents": true
  },
  "mp-alipay": {
    "usingComponents": true
  },
  "mp-baidu": {
    "usingComponents": true
  },
  "mp-toutiao": {
    "usingComponents": true
  },
  "h5": {
    "title": "LunaTV",
    "template": "index.html",
    "router": {
      "mode": "hash"
    },
    "devServer": {
      "port": 5173,
      "host": "0.0.0.0",
      "proxy": {
        "/api": {
          "target": "http://127.0.0.1:3000",
          "changeOrigin": true
        }
      }
    },
    "publicPath": "./",
    "optimization": {
      "treeShaking": {
        "enable": true
      }
    },
    "titleNView": false
  },
  "uniStatistics": {
    "enable": false
  },
  "vueVersion": "3"
}
